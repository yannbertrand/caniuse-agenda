{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:js-lib",
    ":automergeMinor"
  ],
  "commitMessagePrefix": ":pushpin:",
  "packageRules": [
    {
      "matchUpdateTypes": [
        "patch"
      ],
      "automerge": true,
      "matchPackageNames": [
        "/^caniuse-lite$/"
      ]
    }
  ]
}
